<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        DataContext="{Binding Source={StaticResource VMLocator}, Path=MainVM}"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="{Binding Name}" HorizontalAlignment="Left" Margin="140,59,0,0" VerticalAlignment="Top" Height="91" Width="262"/>
        <Button Command="{Binding MyCommande}" Content="Click Click" HorizontalAlignment="Left" Margin="351,175,0,0" VerticalAlignment="Top" Width="170" Height="84"/>
    </Grid>
</Window>
